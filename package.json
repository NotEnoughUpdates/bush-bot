		"rimraf": "^3.0.2",
		"sequelize": "^6.12.4",
		"source-map-support": "^0.5.21",
		"tinycolor2": "^1.4.2",
		"tslib": "^2.3.1",
		"typescript": "^4.5.4",
		"vm2": "^3.9.5"
	},
	"devDependencies": {
		"@types/eslint": "^8",
