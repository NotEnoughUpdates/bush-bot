		"discord-akairo": "npm:@notenoughupdates/discord-akairo@dev",
		"discord-api-types": "0.33.0",
		"discord.js": "npm:@notenoughupdates/discord.js@dev",
		"fuse.js": "^6.6.2",
		"googleapis": "^100.0.0",
		"got": "^12.0.4",
		"lodash": "^4.17.21",
		"mathjs": "^10.5.3",
		"nanoid": "^3.3.4",
		"node-os-utils": "^1.3.6",
		"numeral": "^2.0.6",
		"pg": "^8.7.3",
		"pg-hstore": "^2.3.4",
		"prettier": "^2.6.2",
		"pretty-bytes": "^6.0.0",
		"rimraf": "^3.0.2",
		"sequelize": "6.20.0",
		"tinycolor2": "^1.4.2",
		"typescript": "^4.6.4",
		"vm2": "^3.9.9"
	},
