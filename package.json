		"@notenoughupdates/humanize-duration": "^4.0.1",
		"@notenoughupdates/simplify-number": "^1.0.1",
		"@notenoughupdates/wolfram-alpha-api": "^1.0.1",
		"@sentry/integrations": "^6.19.7",
