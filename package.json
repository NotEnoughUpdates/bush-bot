		"@types/node-os-utils": "^1.2.0",
		"@types/numeral": "^2.0.2",
		"@types/pg": "^8.6.5",
		"@types/prettier": "^2.6.1",
		"@types/rimraf": "^3.0.2",
		"@types/tinycolor2": "^1.4.3",
		"@types/validator": "^13.7.2",
		"@typescript-eslint/eslint-plugin": "^5.26.0",
